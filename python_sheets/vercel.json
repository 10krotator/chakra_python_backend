{
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "main.py"
      }
    ],
    "excludeFiles": [
        "venv/**",
        "tests/**",
        "*.pyc",
        "__pycache__/**",
        ".pytest_cache/**",
        ".git/**"
    ]
}